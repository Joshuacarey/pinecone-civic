{
  "name": "Pinecone-Civic",
  "version": "1.3.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@types/googlemaps": "3.30.8",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "^16.8.23",
    "ajv": "^6.5.4",
    "animate.css": "3.6.1",
    "classnames": "2.2.6",
    "formik": "^1.3.2",
    "gun": "^0.2019.416",
    "history": "4.7.2",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "moment": "2.22.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "4.1.2",
    "password-generator": "^2.2.0",
    "prop-types": "15.6.2",
    "react": "^16.8.6",
    "react-animate-on-scroll": "2.1.4",
    "react-block-ui": "^1.1.3",
    "react-datetime": "2.14.0",
    "react-dom": "^16.8.6",
    "react-google-maps": "9.4.5",
    "react-image-gallery": "0.8.7",
    "react-nouislider": "2.0.1",
    "react-parallax": "1.7.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-modal": "^1.5.2",
    "react-scripts": "1.1.4",
    "react-slick": "0.23.1",
    "react-sortable-hoc": "^1.8.3",
    "react-swipeable-views": "0.12.13",
    "react-tagsinput": "3.19.0",
    "react-text-mask": "^5.4.3",
    "react-toggle-display": "^2.2.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "NODE_PATH=./src npm-run-all -p watch-css start-js",
    "build": "npm run build-css && NODE_PATH=./src react-scripts build",
    "test": "NODE_PATH=./src react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettify": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "typecheck": "tsc",
    "storybook": "NODE_PATH=./src start-storybook -p 9009 -s public",
    "build-storybook": "NODE_PATH=./src build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-viewport": "^4.1.6",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.11",
    "@types/lodash": "^4.14.119",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^6.0.12",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.3",
    "@types/storybook__addon-viewport": "^4.1.0",
    "@types/storybook__react": "^4.0.0",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^1.2.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^16.7.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
